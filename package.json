    "build:server": "node_modules/.bin/cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "lint": "node_modules/.bin/eslint --fix --ext .vue --ext .js assets components helper pages router store",
    "heroku-postbuild": "npm run build && npm run start"
  },
